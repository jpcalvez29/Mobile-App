#:import NoTransition kivy.uix.screenmanager.NoTransition

NavigationLayout:
    MDNavigationDrawer:
        drawer_logo: "image.jpg"
        NavigationDrawerSubheader:
            text: "Lets Get Fit"
        NavigationDrawerIconButton:
            text: "Menu"
            on_release:
                screen_manager.current = "Screen1"
        NavigationDrawerIconButton:
            text: "Beginner"
            on_release:
                screen_manager.current = "Screen2"
        NavigationDrawerIconButton:
            text: "Intermediate"
            on_release:
                screen_manager.current = "Screen3"
        NavigationDrawerIconButton:
            text: "Advanced"
            on_release:
                screen_manager.current = "Screen4"

    BoxLayout:
        orientation: 'vertical'

        MDToolbar:
            title: "30 Day Fitness"
            elevation:10
            md_bg_color: app.theme_cls.primary_color
            left_action_items: [['menu', lambda x: root.toggle_nav_drawer()]]

        ScreenManager:
            id: screen_manager
            transition: NoTransition()

            Screen:
                name: "Screen1"
                orientation: 'vertical'

                canvas.before:
                    Rectangle:
                        pos: self.pos
                        size: self.size
                        source: 'image2.jpg'

